{
  "name": "@se-2/noir",
  "version": "0.0.1",
  "scripts": {
    "circuit-new": "nargo new circuits/$@",
    "circuit-test": "cd $@; nargo test",
    "circuit-generate-solidity-verifier": "cd $@; nargo codegen-verifier",
    "test": "mocha --require ts-node/register --require dotenv/config --timeout 10000 --exit --recursive ./test/**/*.ts",
    "lint": "eslint --config ./.eslintrc.json --ignore-path ./.eslintignore ./*.ts ./deploy/**/*.ts ./ scripts/**/*.ts ./test/**/*.ts",
    "lint-staged": "eslint --config ./.eslintrc.json --ignore-path ./.eslintignore"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/eslint": "^8",
    "@types/mocha": "^9.1.1",
    "@types/prettier": "^2",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "chai": "^4.3.6",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.2.0",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "envfile": "^6.18.0"
  }
}
